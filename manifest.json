{
    "manifest_version": 2,
    "name": "Unit Converter",
    "version": "0.1.0",
    "description": "Convert imperial units to international units",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["unitConverter.css"],
            "js": ["unitConverter.js"],
            "run_at": "document_end"
        }
    ]
}
